<h1>Tipos de usuários</h1>

<table>
  <tr>
    <th>Descrição</th>
    <th></th>
  </tr>

<% @tipo_usuarios.each do |tipo_usuario| %>
  <tr>
    <td><%= tipo_usuario.descricao %></td>
    <td class="botoes"><%= link_to 'Visualizar', tipo_usuario %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo tipo de usuário', new_tipo_usuario_path %>
<%= will_paginate @tipo_usuarios %>