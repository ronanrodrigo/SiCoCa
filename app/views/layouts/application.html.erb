<!DOCTYPE html>
<html>
<head>
  <title>SiCoCa - Sistema de Controle de Campanhas</title>
  <%= stylesheet_link_tag 'reset' %>
  <%= stylesheet_link_tag 'sicoca' %>
  <%= stylesheet_link_tag 'jquery-ui-1.8.5.custom' %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'jquery-1.4.2.min' %>
  <%= javascript_include_tag 'jquery-ui-1.8.5.custom.min' %>
  <%= csrf_meta_tag %>
</head>
<script type="text/javascript"> 
	$(document).ready(function() {
		$( "button, input:submit, a").button();
		$(":text"	).addClass('text ui-widget-content ui-corner-all');
		$(":text"	).css({height: 20})
		$(":select"	).addClass('text ui-widget-content ui-corner-all');
	});
</script>
<body>
	<div class="main">
		<a class="toolbarimg" href="http://localhost:3000">
			<img class="header" src="../../../images/assis.png">
		</a>
		<div class="ui-widget-content ui-corner-all toolbar ui-widget-header">
			<%= link_to "Manter bônus", doacaos_path %>
			<%= link_to "Manter placas", placa_casas_path %>
			<%= link_to "Manter cavaletes", cavaletes_path %>
			<%= link_to "Manter cabos eleitorais", cabo_eleitorals_path %>
			<%= link_to "Manter carros de som", carro_soms_path %>
			<%= link_to "Manter usuários", usuarios_path %>
		</div>
		<div class="ui-widget-content ui-corner-all content">
			<%= yield %>
		</div>
	</div>
</body>
</html>
